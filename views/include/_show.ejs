
<% for(var i=0;i<foodlist.length;i++){ %>

<tr>
    <td><%=i+1%></td>
    <td><%=foodlist[i].food_item%></td>
    <td><%=foodlist[i].ftype%></td>
    <td><%=foodlist[i].fstorage%></td>
    <td><%= foodlist[i].pdate.substring(8, 10) + "-" + foodlist[i].pdate.substring(5, 7) + "-" + foodlist[i].pdate.substring(0, 4)%></td>
    <td><%=foodlist[i].rdate.substring(8, 10) + "-" + foodlist[i].rdate.substring(5, 7) + "-" + foodlist[i].rdate.substring(0, 4)%></td>
    <td><%=foodlist[i].atime%></td>
    <td>
        <a href="/update-food-item?id=<%=foodlist[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id="<%=foodlist[i]._id%>">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>

</tr>
<% } %>

